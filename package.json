  "license": "MIT",
  "bugs": {
    "url": "https://github.com/colinbate/redux-form-submit-saga/issues"
  },
  "homepage": "https://github.com/colinbate/redux-form-submit-saga#readme",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.16.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015-no-commonjs": "0.0.2",
    "cross-env": "^3.1.3",
    "eslint": "^3.0.1"
  },
  "peerDependencies": {
    "redux-form": "6.0.0-rc.2 - 7",
    "redux-saga": "0.11.0 - 0.16.x"
  },
  "dependencies": {
